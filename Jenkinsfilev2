pipeline {
  agent any
   stages {
    stage ('Clean') {
    steps {
        sh '''#!/bin/bash
        scp pkill.sh ubuntu@52.207.250.161:/home/ubuntu
        ssh ubuntu@52.207.250.161 'bash -s </home/ubuntu/pkill.sh' 
        '''
    }
}

stage ('Deploy') {
    steps {
        sh '''#!/bin/bash
        scp setup2.sh ubuntu@52.207.250.161:/home/ubuntu
        ssh ubuntu@52.207.250.161 'bash -s </home/ubuntu/setup2.sh'
        '''
    }
}

  }
}
