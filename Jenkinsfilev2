pipeline {
  agent any
   stages {
    stage ('Clean') {
    steps {
        sh '''#!/bin/bash
        scp pkill.sh ubuntu@10.0.1.33:/home/ubuntu
        ssh ubuntu@10.0.1.33 'bash -s </home/ubuntu/pkill.sh' 
        '''
    }
}

stage ('Deploy') {
    steps {
        sh '''#!/bin/bash
        scp setup2.sh ubuntu@10.0.1.33:/home/ubuntu
        ssh ubuntu@10.0.1.33 'bash -s </home/ubuntu/setup2.sh'
        '''
    }
}

  }
}
